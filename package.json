{
    "name": "dopefolio",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
      "compile:scss": "sass sass/main.scss css/style.css --watch",
      "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.css -o css/style.css",
      "compress:css": "sass sass/main.scss css/style.css --style=compressed --no-source-map",
      "build": "npm run compress:css && npm run prefix:css"
    },
    "author": "",
    "license": "GPL-3.0",
    "devDependencies": {
      "autoprefixer": "^10.3.1",
      "sass": "^1.80.0",
      "npm-run-all": "^4.1.5",
      "postcss-cli": "^8.3.1"
    }
  }